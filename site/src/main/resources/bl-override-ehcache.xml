<ehcache xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    
    <cache
        name="blStandardElements"
        maxElementsInMemory="10000"
        eternal="false"
        overflowToDisk="false"
        timeToLiveSeconds="3">
        <cacheEventListenerFactory class="org.broadleafcommerce.common.cache.engine.HydratedCacheEventListenerFactory"/>
    </cache>
    
    <cache
        name="blOrderElements"
        maxElementsInMemory="10000"
        eternal="false"
        overflowToDisk="false"
        timeToLiveSeconds="3"/>
        
    <defaultCache
        maxElementsInMemory="10000"
        eternal="false"
        overflowToDisk="false"
        timeToLiveSeconds="3"/>
        
    <cache
        name="org.hibernate.cache.StandardQueryCache"
        maxElementsInMemory="10000"
        eternal="false"
        overflowToDisk="false"
        timeToLiveSeconds="5"/>

    <cache
        name="query.Catalog"
        maxElementsInMemory="10000"
        eternal="false"
        overflowToDisk="false"
        timeToLiveSeconds="5"/>

    <cache name="cmsPageCache"
        maxElementsInMemory="10000"
        eternal="false"
        overflowToDisk="false"
        timeToLiveSeconds="3"/>

    <cache name="cmsStructuredContentCache"
        maxElementsInMemory="10000"
        eternal="false"
        overflowToDisk="false"
        timeToLiveSeconds="3"/>
        
    <cache name="blCMSElements"
        maxElementsInMemory="10000"
        eternal="false"
        overflowToDisk="false"
        timeToLiveSeconds="3"/>
        
    <cache
        name="blTranslationElements"
        maxElementsInMemory="10000"
        eternal="false"
        overflowToDisk="false"
        timeToLiveSeconds="5"/>
        
</ehcache>